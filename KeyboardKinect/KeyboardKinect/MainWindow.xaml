<Window x:Class="KeyboardKinect.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyboardKinect"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1920">
    <Grid Height="424" Width="512">
        <Image Name="camera"/>
        <InkCanvas Name="canvas" Background="{x:Null}"/>
        <Button x:Name="addKeyButton" Content="Add Key" HorizontalAlignment="Left" Height="20" Margin="0,429,0,-25" VerticalAlignment="Top" Width="59" Click="addKeyButton_Click"/>
        <Button x:Name="saveKeyboardButton" Content="Save Keyboard" HorizontalAlignment="Left" Height="20" Margin="64,429,0,-25" VerticalAlignment="Top" Width="101"/>
        <Button x:Name="clearButton" Content="Clear" HorizontalAlignment="Left" Height="20" Margin="170,429,0,-25" VerticalAlignment="Top" Width="40"/>
    </Grid>
</Window>
